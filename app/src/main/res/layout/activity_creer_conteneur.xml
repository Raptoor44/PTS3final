<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Conteneur.Creer_conteneur">

    <FrameLayout
        android:id="@+id/frameLayout12"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="1dp"
        android:layout_marginTop="1dp"
        android:layout_marginEnd="1dp"
        android:layout_marginBottom="1dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <ImageButton
                android:id="@+id/id_activity_creer_conteneur_bouton_retour"
                android:layout_width="92dp"
                android:layout_height="52dp"
                app:srcCompat="@drawable/fleche_retour" />

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="251dp">

            </FrameLayout>

            <com.google.android.material.textfield.TextInputLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <com.google.android.material.textfield.TextInputEditText
                    android:id="@+id/id_activity_creer_conteneur_text_input"
                    android:layout_width="314dp"
                    android:layout_height="wrap_content"
                    android:hint="Nom du conteneur" />

                <Button
                    android:id="@+id/id_activity_creer_conteneur_bouton_creer_conteneur"
                    android:layout_width="315dp"
                    android:layout_height="wrap_content"
                    android:text="ajouter" />
            </com.google.android.material.textfield.TextInputLayout>
        </LinearLayout>
    </FrameLayout>
</androidx.constraintlayout.widget.ConstraintLayout>